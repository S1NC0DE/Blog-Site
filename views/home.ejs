<%- include('partials/header') -%>
<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach(function(post) { %>
    <h1><%= post.title %></h1>
    <p>
            <%= post.content.slice(0,100).concat('...') %>
            <a href="/posts/<%= post.title %>">Read more</a>
    </p>
    
    
<% }) %>


<%- include('partials/footer') -%>